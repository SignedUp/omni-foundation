<UserControl x:Class="Omniscient.Foundation.Contrib.Silverlight.ViewNavigator"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" 
    xmlns:tk="clr-namespace:Microsoft.Windows.Controls;assembly=Microsoft.Windows.Controls">
    <StackPanel>
        <StackPanel Orientation="Horizontal">
            <Button Content="Back" x:Name="ButtonBack" Click="ButtonBack_Click" />
            <Button Content="Forward" x:Name="ButtonForward" Click="ButtonForward_Click" />
        </StackPanel>
        <StackPanel Orientation="Horizontal" x:Name="StackPanelContent" Height="50" />
    </StackPanel>
</UserControl>
