<UserControl x:Class="Omniscient.Foundation.Contrib.Silverlight.ViewNavigator"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" 
    xmlns:tk="clr-namespace:Microsoft.Windows.Controls;assembly=Microsoft.Windows.Controls">
    <StackPanel Orientation="Horizontal">
        <Button x:Name="ButtonBack" Click="ButtonBack_Click" tk:DockPanel.Dock="Left" Width="30" Height="50" VerticalAlignment="Center" HorizontalAlignment="Center" Margin="3">
            <Button.Content>
                <Image Width="20" Height="20" Source="Omniscient.Foundation.Contrib.Silverlight;component/Resources/control_rewind_blue.png" />
            </Button.Content>
        </Button>
        <Button x:Name="ButtonForward" Click="ButtonForward_Click" tk:DockPanel.Dock="Right" Width="30" Height="50" VerticalAlignment="Center" HorizontalAlignment="Center" Margin="3">
            <Button.Content>
                <Image Width="20" Height="20" Source="Omniscient.Foundation.Contrib.Silverlight;component/Resources/control_fastforward_blue.png" />
            </Button.Content>
        </Button>
        <StackPanel Orientation="Horizontal" x:Name="StackPanelContent" Margin="3" />
    </StackPanel>
</UserControl>
